{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "defexception",
    "defprotocol",
    "defstruct",
    "defdelegate",
    "def.+(.+\\\\.+).+do",
    "^\\s+use\\s+"
  ],
  "custom_stop_words": [
    "message\\s->",
    "error\\s->"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": false
  }
}
